# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
max_line_length = 120
charset = utf-8

# Google style 2 space indentation
[*.py]
indent_style = space
indent_size = 4
